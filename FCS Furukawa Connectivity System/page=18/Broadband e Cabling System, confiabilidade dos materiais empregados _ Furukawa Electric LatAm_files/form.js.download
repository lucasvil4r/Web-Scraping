 $(document).ready(function() {

     $(function() {
         $.mask.definitions['~'] = "[+-]";
         $("#date").mask("99/99/9999", {
             placeholder: "mm/dd/yyyy",
             completed: function() {
                 alert("completed!");
             }
         });
         $("#phoneExt").mask("(999) 999-9999? x99999");
         $("#tin").mask("99-9999999");
         $("#ssn").mask("999-99-9999");
         $("#product").mask("a*-999-a999", {
             placeholder: " "
         });
         $("#eyescript").mask("~9.99 ~9.99 999");
         $("#po").mask("PO: aaa-999-***");
         $("#pct").mask("99%");
         $("#phoneAutoclearFalse").mask("(999) 999-9999", {
             autoclear: false,
             completed: function() {
                 alert("completed autoclear!");
             }
         });
         $("#phoneExtAutoclearFalse").mask("(999) 999-9999? x99999", {
             autoclear: false
         });
         
         /* Furukawa */
         
         $(".mobilePhone").mask("+99 99 99999 9999");
         $(".telefone").mask("+99 99 ?9999 9999");
         $(".cel").mask("+99 99 99999 9999");
         $(".cep").mask("99999-999");
         
         

         $("input").blur(function() {
             $("#info").html("Unmasked value: " + $(this).mask());
         }).dblclick(function() {
             $(this).unmask();
         });
     });

 });